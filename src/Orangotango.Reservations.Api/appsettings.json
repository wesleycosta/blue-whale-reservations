{
  "Service": {
    "Name": "orangotango-reservations",
    "Version": "1.0.0"
  },
  "AllowedHosts": "*",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Elasticsearch"
      }
    ],
    "Enrich": [ "FromLogContext" ]
  }
}
