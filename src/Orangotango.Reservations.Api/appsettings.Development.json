{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=(LocalDb)\\MSSQLLocalDB;Initial Catalog=reservations;Integrated Security=SSPI;"
  },
  "Elasticsearch": {
    "Uri": "http://localhost:9200",
    "IndexFormat": "orangotango-reservations-{0:yyyy.MM}"
  },
  "RabbitMQ": {
    "HostName": "localhost",
    "VirtualHost": "/",
    "Username": "guest",
    "Password": "guest"
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Elasticsearch"
      }
    ],
    "Enrich": [ "FromLogContext" ]
  }
}
